<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hackerda.platform.mapper.ext.CourseExtMapper">

    <select id="selectByCourseList" resultMap="com.hackerda.platform.mapper.CourseMapper.BaseResultMap" parameterType="java.util.List" >
        select
               <include refid="com.hackerda.platform.mapper.CourseMapper.Base_Column_List" />
        from course

        where (num, course_order, term_year, term_order) in

        <foreach collection="list" item="item" index="index" open="(" close=")" separator=",">
            (
            #{item.num,jdbcType=INTEGER},
            #{item.courseOrder,jdbcType=VARCHAR},
            #{item.termYear,jdbcType=VARCHAR},
            #{item.termOrder,jdbcType=INTEGER}
            )
        </foreach>
    </select>
</mapper>